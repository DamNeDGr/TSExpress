
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
	id Int @id @default(autoincrement())
	username String @unique
	email String @unique
	password String
	created_at DateTime @default(now())
}
